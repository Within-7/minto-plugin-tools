{
  "manifest_version": 3,
  "name": "Service Worker Extension",
  "description": "Demonstrates background service worker capabilities",
  "version": "1.0.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Service Worker Demo"
  },
  "permissions": ["storage", "alarms", "notifications"]
}